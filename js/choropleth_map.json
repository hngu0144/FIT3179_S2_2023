{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "data": {
      "url": "data/ne_110m_admin_0_countries.json",
      "format": {
        "type": "topojson",
        "feature": "counties"
      }
    },
    "transform": [{
      "lookup": "id",
      "from": {
        "data": {
          "url": "data/data.csv"
        },
        "key": "id",
        "fields": ["rate"]
      }
    }],
    "projection": {
      "type": "albersUsa"
    },
    "mark": "geoshape",
    "encoding": {
      "color": {
        "field": "rate",
        "type": "quantitative"
      }
    }
  }
  
  