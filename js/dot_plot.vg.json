{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title":"Country's Mean Tertiary and education enrolment",
  "width": 800,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/hngu0144/FIT3179_S2_2023/main/data/data.csv"
  },
  
  "repeat": {
    "layer": ["gross_tertiary_education_enrollment", "gross_primary_education_enrollment_country"]
  },
  "spec": {
    "mark": "circle",
    "encoding": {
      "x": {
        "field": "country",
        "type": "ordinal",
        "title": "Country"
      },
      "y": {
        "aggregate": "mean",
        "field": {"repeat": "layer"},
        "type": "quantitative",
        "title": "Mean Tertiary and education enrolment",
        "sort": "-x"
      },
      "color": {
        "datum": {"repeat": "layer"},
        "type": "nominal",
        "legend": null
      },
      "tooltip": [
    {
      "field": "country",
      "type": "nominal",
      "title": "Country"
    },
    {
      "field": "gross_tertiary_education_enrollment",
      "type": "quantitative",
      "title": "Mean Gross Tertiary Education Enrolment"
    },
    {
      "field": "gross_primary_education_enrollment_country",
      "type": "quantitative",
      "title": "Mean Gross Primary Education Enrolment"
    }
  ]
    }
  }
}
