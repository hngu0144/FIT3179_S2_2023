{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title":"Country's GDP, CPI and Tax rate",
    "width": 600,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/hngu0144/FIT3179_S2_2023/main/data/data.csv"
    },
    
    "repeat": {
      "layer": ["gross_tertiary_education_enrollment", "gross_primary_education_enrollment_country"]
    },
    "spec": {
      "mark": "circle",
      "encoding": {
        "x": {
          "field": "country",
          "type": "ordinal"
        },
        "y": {
          "aggregate": "mean",
          "field": {"repeat": "layer"},
          "type": "quantitative",
          "title": "Mean of US and Worldwide Gross",
          "sort": "-x"
        },
        "color": {
          "datum": {"repeat": "layer"},
          "type": "nominal"
        }
      }
    }
  }
  